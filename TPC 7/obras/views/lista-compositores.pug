extends layout
block content
    .w3-container
        h1 Lista de compositores
        table.w3-table-all
            tr
                th Nome
                th Numero de obras
                each comp in lista
                    tr
                        td= comp._id
                        td= comp.numObras
                       
    footer.w3-container.w3-teal
        address Gerado por Gestao de Obras App:
            a(href='/')  Voltar